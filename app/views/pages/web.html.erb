</div>

<hr>



  <!-- projects cards -->

  <!-- unlisenced -->
  <div id="projects">
    <h2 class="pre text-center">Portfolio</h2>
    <div class="project">
      <%= image_tag "unlicensed.png", class: "unlisenced-image"%>
      <div class="content">
        <h3>
          The Unlicensed
          <span>2020</span>
        </h3>
        <p>A web app for individuals that doesn’t have the chance to practise their skills due to workforce’s barriers. It provides a marketplace where people can provide  and book services.</p>
          <div class="links">
            <button class="btn-white">Learn More</button>
            <%= link_to "https://unlicensed.herokuapp.com/", :target => "_blank" do %>
              <button class="btn-black">Visit Site</button>
            <% end %>
          </div>
        </div>
      </div>
    </div>
    <hr>

  <!-- day-o -->
    <div class="project">
      <%= image_tag "dayO.png", class: "dayo-image"%>
      <div class="content">
        <h3>
          Day 0
          <span>2020</span>
        </h3>
        <p>A web app that help you build eco conscience habit through challenges, track your impact and connect you to a community that shares the same goals.</p>
        <div class="links">
          <button class="btn-white">Learn More</button>
          <%= link_to "http://www.day-o.cc/", :target => "_blank" do %>
            <button class="btn-black">Visit Site</button>
          <% end %>
        </div>
      </div>
    </div>

    <hr>

    <div class="contact">
      <h2 class="text-center mb-5">Let's Get In Touch!</h2>
      <form method="post" action="mailto:noursouidd@gmail.com">
          <div class="form-row mb-3">
            <div class="col">
              <label for="name">Name</label>
              <input type="text" class="form-control" id="name" placeholder="Name">
            </div>
            <div class="col">
              <label for="Email">Email</label>
              <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Email">
            </div>
          </div>
          <div class="form-group">
              <label for="message">Your message</label>
              <textarea class="form-control" id="message" rows="3"></textarea>
            </div>
          <button type="submit" class="w-96 btn-black">Send</button>
        </form>
      </div>


      <div class="social-links" id="contact">
        <ul class="list-inline">
          <li>
            <a href="https://github.com/NourSouidd" target="_blank" class="social-icons">
              <i class="fab fa-github-alt"></i>
            </a>
          </li>
          <li>
            <a href="https://www.linkedin.com/in/nour-souid-bbb9611a2/" target="_blank" class="social-icons">
              <i class="fab fa-linkedin"></i>
            </a>
          </li>
        </ul>
      </div>
  </div>
</div>
